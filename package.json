{
  "name": "nuxtlabs",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "start": "nuxt start",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:css",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix && npm run lint:css:fix",
    "lint:js": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint '**/*.{js,ts,vue}' --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --fix '**/*.{js,ts,vue}' --ignore-path .gitignore",
    "lint:prettier": "prettier --check '**/*.{js,ts,vue}' --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write '**/*.{js,ts,vue}' --ignore-path .gitignore",
    "lint:css": "stylelint '**/*.{css,vue}' --ignore-path .gitignore",
    "lint:css:fix": "stylelint --fix '**/*.{css,vue}' --ignore-path .gitignore"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@docus/app": "^1.3.5",
    "@docus/theme": "^1.2.2",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "vue-plausible": "^1.2.1"
  },
  "dependencies": {
    "@vueuse/motion": "^2.0.0-beta.1"
  }
}