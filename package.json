{
  "name": "nuxtlabs",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "start": "nuxt start",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:js": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint '**/*.{js,ts,vue}' --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --fix '**/*.{js,ts,vue}' --ignore-path .gitignore",
    "lint:prettier": "prettier --check '**/*.{js,ts,vue}' --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write '**/*.{js,ts,vue}' --ignore-path .gitignore",
    "lint:css": "stylelint '**/*.{css,vue}' --ignore-path .gitignore",
    "lint:css:fix": "stylelint --fix '**/*.{css,vue}' --ignore-path .gitignore"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxt/typescript-build": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@docus/app": "^1.3.5",
    "@docus/theme": "^1.2.2",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^37.1.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "nuxt-edge": "latest",
    "prettier": "^2.5.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.0",
    "nuxt-use-motion": "2.0.0-beta.8",
    "vue-clickaway": "^2.2.2",
    "vue-plausible": "^1.3.1"
  }
}
